public class SaleTicket {
	public static void main(String[] args) {
		Ticket ticket = new Ticket(100);
		Thread thread = new Thread(ticket);
		Thread thread2 = new Thread(ticket);
		thread.setName("窗口1");
		thread2.setName("窗口2");
		thread2.start();
		thread.start();
	}
}
